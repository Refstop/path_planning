function drawCoord = drawCoord(map, reference_coord, target_coord)
    T_or = rot2d(reference_coord);
    T_rt = rot2d(target_coord);
    origin = [0;0;1];
    for i = 1:5
        origin(:,end+1) = []
    end
    T_ot = T_or*T_rt*
    for i = 1:5
        setOccupancy(map, -[origin(1)-0.05*i, origin(2)], 1);
        setOccupancy(map, -[origin(1), origin(2)-0.05*i], 1);
    end
end